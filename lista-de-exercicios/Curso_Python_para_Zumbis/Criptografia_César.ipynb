   "source": [
    "print('***Criptografia de Mensagens***')\n",
    "\n",
    "def esconde(msg):\n",
    "    s = ''\n",
    "    for c in msg:\n",
    "        s += chr(ord(c) + 30000)\n",
    "    return s\n",
    "def mostra(msg):\n",
    "    s = ''\n",
    "    for c in msg:\n",
    "        s += chr(ord(c) - 30000)\n",
    "    return s\n",
    "\n",
    "print(esconde('Olá mundo!'))\n",
    "\n",
    "print(mostra('畿疜瘑畐疝疥疞疔疟畑'))\n"
   ]
  }
